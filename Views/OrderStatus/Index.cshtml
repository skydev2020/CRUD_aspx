@model IEnumerable<EF_CRUD.Models.OrderStatu>

@{
    ViewBag.Title = "Order Status";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StatusType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StatusType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OrderStatusId }) |
            @Html.ActionLink("Details", "Details", new { id=item.OrderStatusId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.OrderStatusId })
        </td>
    </tr>
}

</table>
